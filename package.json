{
  "name": "poches-et-fils",
  "version": "0.0.1",
  "scripts": {
    "watch": "concurrently \"theme watch\" \"parcel watch src/scripts/theme.js --out-dir src/assets --out-file scripts.js\"",
    "build": "parcel build src/scripts/theme.js --out-dir src/assets --out-file scripts.js"
  },
  "xo": {
    "plugins": [
      "babel"
    ],
    "envs": [
      "browser"
    ],
    "globals": [
      "fetch",
      "$",
      "slate"
    ],
    "rules": {
      "import/first": 0,
      "import/no-unassigned-import": 0
    }
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "concurrently": "4.0.1",
    "eslint-plugin-babel": "5.2.0",
    "node-sass": "4.9.3",
    "parcel-bundler": "^1.10.3",
    "xo": "0.23.0"
  },
  "dependencies": {
    "@shopify/slate": "^0.14.0",
    "@shopify/themekit": "^1.0.1",
    "@sindresorhus/slugify": "0.3.0",
    "algoliasearch": "3.30.0",
    "global": "^4.3.2",
    "js-search": "1.4.2",
    "query-string": "6.1.0",
    "scroll": "2.0.3"
  }
}
