
  <div class="cs-faq-section">
    <h1 class="faq-heading">FAQ</h1>
    <div class="form-block w-form">

      <form action="/search" class="cs-faq-form">
       <input class="cs-faq-text-field w-input" maxlength="256" autofocus="true" type="text" placeholder="Search" name="q" value="{{ search.terms | escape }}"  placeholder="What can we help you with ?"  />
        <input hidden type="submit" value="Search" />
      </form>
    </div>
  </div>

  <div class="cs-faq-content-section">
    <div class="cs-faq-side-info">
      <div class="div-block-3">
        <div class="faq-questions-live-chat">
          <h4 class="faq-sub-heading">Most Popular Questions</h4><a href="#" class="faq-link">Products</a><a href="#" class="faq-link">Payments</a><a href="#" class="faq-link">Orders</a><a href="#" class="faq-link">Shipping</a><a href="#" class="faq-link">Returns</a><a href="#" class="faq-link">Other</a>
          <div class="need-help">Need Help ?</div>
          <div class="need-help-subheading">This is some text inside of a div block.</div><a href="#" class="live-chat-button w-button">Live Chat</a></div>
        <div class="faq-questions-live-chat light">
          <h2>Contact Us</h2>
          <h5 class="company-name">Poches ET Fils hq</h5>
          <p>2005 Street Name,<br>City, State, <br>Country<br><br>Tel: 555-555-5555<br>info@business.com</p>
          <h5 class="company-name">Where to find us</h5>
          <p>2005 Street Name,<br>City, State, <br>Country<br><br>Tel: 555-555-5555<br>info@business.com</p>
        </div>
      </div>
      <div class="div-block-4">
        <div class="products-faq-toggle">
          <h1 class="toggle-heading">Products</h1>
          <div class="toggles--section">


          {% for article in blog.articles %}

            <div class="toggle--item">
              <div class="toggle--activation" data-ix="toggle-interaction">
                <h4 class="toggle--heading">{{ article.title }}</h4><img src="images/minus.svg" class="toggle--icon--minus" data-ix="originally-hidden"><img src="images/plus.svg" class="toggle--icon--plus"></div>
              <div class="toggle--contents">
               {% if article.excerpt.size > 0 %}
        {{ article.excerpt }}
      {% else %}
        <p>
          {{ article.content | strip_html | truncatewords: 100 }}
        </p>
      {% endif %}

      <a href="{{ article.url }}"> Read more</a>
              </div>
            </div>


            {% endfor %}
            
          </div>
        </div>
        <div class="cs-map-block">
          <div class="cs-map w-widget w-widget-map"></div>
        </div>
      </div>
    </div>
  </div>


