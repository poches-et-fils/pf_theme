/* DESKTOP STYLING ONLY */

ul.colors-container {
  list-style: none;
  margin-top: 5px;
  padding: 0;
  display: flex;
  justify-content: space-evenly;
}
ul.colors-container li {
  float: left;
  margin: 3px;
  cursor: pointer;
  height: 25px;
  width: 25px;
  border-radius: 50%;
  border: 1px solid rgba(128, 128, 128, 0.49);
  transition: 0.3s;
}

ul.colors-container li:hover {
  opacity: 0.5;
  transition: 0.3s;
}

ul.colors-container li.selected-color {
  border: 2px solid rgba(0, 0, 0, 0.54);
  transition: 0.3s;
}

li[title="Black"] { background: #161616; }
li[title="White"] { background: white; }
li[title="Navy"] { background: navy; }
li[title="Grey"] { background: #eaeaea; }
li[title="Burgundy"] { background: #900020; }
li[title="Dark-Grey"] { background: #898686; }

@media screen and (min-width: 901px) {
  /* These should only affect desktop */
  .ajax--cart--item {
    padding-right: 20px;
  }

  .ajax--cart--lower {
    padding-top: 8px;
  }

  /* The remove icon for the items should only appear when you hover over the item, so it is initally hidden */
  .item-start:hover .remove {
    opacity: 1;
    transition: 0.3s;
  }
  .ajax--cart--item--quantity.remove {
    opacity: 0;
    font-size: 25px!important;
    border: none!important;
    transition: 0.3s;
  }

  .ajax--cart--item--quantity { border: 2px solid lightgray; }

  .ajax--cart--free-shipping {
    font-size: 24px!important;
    font-weight: normal;
    font-style: normal;
    font-stretch: normal;
    line-height: 1.33;
    letter-spacing: normal;
    text-align: left;
    color: #161616;
  }

  .ajax--cart--bottom { padding-top: 40px; }

  .ajax--cart--subtotal { padding-top:30px!important; }

